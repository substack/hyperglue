{
    "name": "hyperglue",
    "version": "0.2.7",
    "description": "update html elements by mapping query selectors to attributes, text, and hypertext",
    "browser": "browser.js",
    "main": "index.js",
    "dependencies": {
        "trumpet": "~0.3.4",
        "ent": "~0.0.5",
        "concat-stream": "~0.1.1"
    },
